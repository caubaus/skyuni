<div class="row">
    <div class="col m3 center">

      <br><br>
      <h5 style="color: #01579b">Sign In</h5>
      <br><br>

      <%= link_to "Google", user_google_oauth2_omniauth_authorize_path, class: "btn red-text white" %>
      <%= link_to "Github", user_github_omniauth_authorize_path, class: "btn black-text white" %>
      <%= link_to "Facebook", user_facebook_omniauth_authorize_path, class: "btn blue-text white" %>


      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= render partial: "shared/devisemes" %>

        <div class="input-field">
          <%= f.email_field :email, :id => "email", :class => "validate", :placeholder => "Email" %>
        </div>

        <div class="input-field">
          <%= f.password_field :password, :id => "email", :class => "validate", :placeholder => "Password", autocomplete: "off" %>
        </div>

        <button class="btn waves-effect waves-light white light-blue-text text-darken-4" type="submit" name="action">Sign In</button>
        <%= link_to "Forgot Password", new_user_password_path, :class => "btn waves-effect waves-light white light-blue-text text-darken-4" %>
      <% end %>
    </div>



    <div class="col m9 offset-s1">
      <script src="https://fast.wistia.com/embed/medias/kme60hjo5e.jsonp" async></script><script src="https://fast.wistia.com/assets/external/E-v1.js" async></script><div class="wistia_responsive_padding" style="padding:56.25% 0 0 0;position:relative;"><div class="wistia_responsive_wrapper" style="height:100%;left:0;position:absolute;top:0;width:100%;"><div class="wistia_embed wistia_async_kme60hjo5e videoFoam=true" style="height:100%;position:relative;width:100%">&nbsp;</div></div></div>
    </div>
</div>
